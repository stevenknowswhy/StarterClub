{
    "compilerOptions": {
        "target": "ES2020",
        "lib": [
            "ES2020"
        ],
        "module": "CommonJS",
        "skipLibCheck": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "baseUrl": "."
    },
    "include": [
        "src"
    ]
}